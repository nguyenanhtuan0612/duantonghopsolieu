                  <td>{{vndFormat mn_duan2}}</td>
                  <td><span class="badge bg-{{setColor per_mn_duan2}}">{{per_mn_duan2}}%</span></td>
                </tr>
          </table>
        </div>
      </div>
      <!-- /.box-body -->
    </div>

